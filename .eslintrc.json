{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "no-restricted-syntax": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "no-plusplus": 0,
        "no-bitwise": 0,
        "react/prop-types": 0
    }
}
